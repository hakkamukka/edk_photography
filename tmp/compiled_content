I"/stylesheet/;T{;{;I">* {
  margin: 0;
  padding: 0;

  font-family: Georgia, Palatino, Times, 'Times New Roman', sans-serif;
}

body {
  background: #fff;
}

a {
  text-decoration: none;
}

a:link,
a:visited {
  color: #f30;
}

a:hover {
  color: #f90;
}

#main {
  position: absolute;

  top: 40px;
  left: 280px;

  width: 640px;
}

#main h1 {
  font-size: 40px;
  font-weight: normal;

  line-height: 40px;

  letter-spacing: -1px;
}

#main h2 {
  font-size: 30px;
  font-weight: normal;

  line-height: 80px;
}

#main h3 {
  font-size: 20px;
  font-weight: normal;
}

#main h4 {
  font-size: 12px;
  font-weight: normal;

  text-align: right;
}

#main p {
  margin: 20px 0;
  
  font-size: 15px;
  
  line-height: 20px;

  text-align: justify;
}

#main ul, #main ol {
  margin: 20px;
}

#main li {
  font-size: 15px;
  
  line-height: 20px;
}

#main ul li {
  list-style-type: square;
}

#article-footer {
  position: absolute;

  width: 640px;
}

#article-footer p {
  line-height: 30px;
}

#content-footer {
  position: absolute;

  left: 0px;
  width: 640px;
}

#content-footer p {
  font-size: 10px;
  line-height: 20px;
  text-align: center;
}

#share {
  position: absolute;

  text-align: center;
}

#sidebar {
  position: absolute;

  top: 40px;
  left: 20px;
  width: 200px;

  padding: 20px 20px 0 0;

  border-right: 1px solid #ccc;

  text-align: right;
}

#sidebar h2 {
  text-transform: uppercase;

  font-size: 13px;

  color: #333;

  letter-spacing: 1px;

  line-height: 20px;
}

#sidebar ul {
  list-style-type: none;

  margin: 20px 0;
}

#sidebar li {
  font-size: 14px;

  line-height: 20px;
}
